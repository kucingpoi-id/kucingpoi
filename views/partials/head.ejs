<% } else if( tmpl == 'list') { %>
    <!-- ANIME LIST -->
    <script>
        $(document).ready(function(){
            var pagelong    = Math.ceil(Math.floor('<%= totalpage %>') / 30);
            var cpp         = Math.floor('<%= forcheck %>') == 0 ? 1 : Math.floor("<%= forcheck %>");
            var kcp         = 2;

            $('.pagination').append('<li class="page-item'+ (cpp == 1 ? ' disabled' : '')+'"><a class="page-link" href="'+ (cpp == 1 ? '#' : '/anime?'+(cpp-1))+'">PREV</a></li>');
            for (let i = 0; i < pagelong; i++) {

                if( (cpp+kcp) ) {
                    if(i == 0 || i == (pagelong-1) ){
                        if ( (cpp == pagelong && i == (pagelong-1)) || (cpp == 1 && i == 0 ) ) {
                            var pagination = '<li class="page-item disabled">'+
                            '<a class="page-link" href="#">'+(i+1)+'</a>'+
                            '</li>';
                        }
                        else {
                            var pagination = '<li class="page-item">'+
                            '<a href="/anime'+ ((i == 0 ) ? '' : '?'+(i+1))+'" class="page-link">'+(i+1)+'</a>'+
                            '</li>';
                        }
                    }
                    else if ( i > (cpp-kcp-2) && i < cpp+kcp ) {
                        if(i == (cpp -1)) {
                            var pagination = 
                            '<li class="page-item active" aria-current="page">'+
                            '<a class="page-link" href="#">'+(i+1)+'</a>'+
                            '</li>';
                        }
                        else {
                            var pagination = '<li class="page-item">'+
                            '<a href="/anime'+ ((i == 0 ) ? '' : '?'+(i+1))+'" class="page-link">'+(i+1)+'</a>'+
                            '</li>';
                        }
                    }
                    else if( i == (cpp+kcp) || i == (cpp-kcp-2) ) {
                        var pagination = 
                        '<li class="page-item disabled">'+
                        '<a class="page-link" href="#">...</a>'+
                        '</li>';
                    }
                    else {  
                        var pagination = '';
                    }

                }
                if(i != pagelong){
                    $('.pagination').append(pagination + " ");
                }
            }
            $('.pagination').append('<li class="page-item'+ (cpp == pagelong ? ' disabled' : '')+'"><a class="page-link" href="'+ (cpp == pagelong ? '#' : '/anime?'+(cpp+1))+'" >NEXT</a></li>');
        })
    </script>
