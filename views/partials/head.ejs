<%  } else if( tmpl == 'genre') { %>
    <script>
    $(document).ready(function(){
        $.get("/cache/genre-<%= paramtitle %>", function(result, status){
            var posts =  $('#post-anime');
            var b = JSON.parse(result);
            var max_size=b.length;
            var sta = 0;
            var elements_per_page = 20;
            if (max_size <= elements_per_page) {
                $("#nextValue,#PreeValue").addClass('disabled')
            }
            var limit = elements_per_page;
            // console.log(max_size)

            goFun(sta,limit);
            function goFun(sta,limit) {
                for (var i =sta ; i < limit; i++) {
                    if (b[i] !== undefined) {
                        var $nr = `
                        <div class="col colek">
                        <a href="/anime/${b[i].link}" class="link-post text-decoration-none" >
                            <div class="tx-container ">
                            <img src="/assets/images/${b[i].thumbnail}" 
                            class="thumbnail mx-auto w-100" width="150" alt="${b[i].judul}"> 
                            <div class="tx-top-right p-1 rounded-bottom text-white">${b[i].type}</div>
                            <div class="tx-bottom-left py-1"><b>Episode 1</b></div>
                            <div class="tx-bottom-right py-1">${b[i].score}</div>
                            <div class="tx-centered rounded-circle"></div>
                            </div>
                            <p class="w-100 text-wrap text-center" style="width: 150px;height:40px;white-space: nowrap;overflow: hidden;text-overflow: ellipsis;font-size: 14px;"><small><b>${b[i].judul}</b></small></p>
                        </a>
                        </div>
                        `;
                        posts.append($nr);
                    }
                }
            }

            $('#nextValue').click(function(e){
                $('#PreeValue').removeClass('disabled')
                var ddsed = $(this).hasClass('disabled')
                if(ddsed) {
                    e.preventDefault();
                }
                var next = limit;
                if(max_size>=next) {
                    limit = limit+elements_per_page;
                    posts.empty();
                    goFun(next,limit);
                }
                else $(this).addClass('disabled')
            });
            $('#PreeValue').click(function(e){
                $('#nextValue').removeClass('disabled')
                var ddsed = $(this).hasClass('disabled')
                    if(ddsed) {
                    e.preventDefault();
                }
                var pre = limit-(2*elements_per_page);
                if(pre>=0) {
                    limit = limit-elements_per_page;
                    posts.empty();
                    goFun(pre,limit);
                    if (pre < 1) {
                        $(this).addClass('disabled')
                    }
                }
                else {
                    $(this).addClass('disabled')
                }
            });
        })

    });
    </script>
